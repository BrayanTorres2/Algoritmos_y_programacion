import pywhatkit 

# Usamos Un try-except
try: 

  # Enviamos el mensaje

  pywhatkit.sendwhatmsg("+57numero",  
                        "Mensaje De Prueba",
                        13,10) 

  print("Mensaje Enviado") 

  

except: 

  print("Ocurrio Un Error")
